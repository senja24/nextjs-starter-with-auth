{
  "name": "nextjs-starter-template",
  "description": "Starter templates for Next.js 14 full-stack projects. Built with Tailwind CSS, ShadCn, authentication, and database. Several branches contain several different configurations, including serverless databases like PostgreSQL with Neon, or MySQL with PlanetScale, Drizzle ORM, Prisma ORM v.5 (serverless-compatible), but also MongoDB and Supabase.",
  "version": "0.1.0",
  "private": true,
  "author": {
    "name": "Ika Agus setiawan",
    "url": "https://ikaagussetiawan.com"
  },
  "scripts": {
    "dev": "contentlayer build && next dev --experimental-https",
    "build": "contentlayer build && next build",
    "start": "next start",
    "postinstall": "bunx prisma generate",
    "db:generate": "bunx prisma generate",
    "db:migrate": "bunx prisma migrate dev",
    "db:push": "pnpm prisma db push",
    "db:pull": "npx prisma migrate reset",
    "db:studio": "npx prisma studio",
    "shadcn:add": "pnpm dlx shadcn-ui@latest add",
    "packages:update-all": "pnpm update --latest",
    "prettier:format": "prettier --write \"**/*.{ts,tsx,mdx}\" --cache",
    "prettier:check": "prettier --check \"**/*.{ts,tsx,mdx}\" --cache",
    "typecheck": "tsc --noEmit",
    "lint": "contentlayer build && next lint",
    "lint:fix": "contentlayer build && next lint --fix"
  },
  "dependencies": {
    "@auth/core": "^0.30.0",
    "@auth/prisma-adapter": "^2.0.0",
